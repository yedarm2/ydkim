		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jest": "26.2.2",
		"postcss": "8.4.14",
		"postcss-preset-env": "7.7.0",
		"postcss-reporter": "7.0.5",
		"sass": "1.52.1",
		"stylelint": "14.8.5",
		"stylelint-config-standard-scss": "4.0.0",
		"stylelint-no-unsupported-browser-features": "5.0.3",
