		"@reduxjs/toolkit": "1.8.2",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.3.0",
		"@testing-library/user-event": "14.2.0",
		"@types/jest": "27.5.0",
		"@types/node": "17.0.38",
		"@types/react": "18.0.10",
		"@types/react-dom": "18.0.5",
		"classnames": "2.3.1",
		"facepaint": "1.2.1",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-redux": "8.0.2",
