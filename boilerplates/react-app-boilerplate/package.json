{
	"name": "react-app-boilerplate",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "react-scripts build",
		"clean": "npx rimraf node_modules && npx rimraf dist && npx rimraf .next",
		"deploy": "echo \"deploy\"",
		"eject": "react-scripts eject",
		"lint": "echo \"no lint\"",
		"serve": "npm start",
		"start": "react-scripts start",
		"test": "react-scripts test"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
