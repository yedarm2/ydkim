		"@emotion/styled": "11.10.4",
		"@reduxjs/toolkit": "1.8.6",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "14.4.3",
		"@types/jest": "29.1.2",
		"@types/node": "18.8.5",
		"@types/react": "18.0.21",
		"@types/react-dom": "18.0.6",
		"classnames": "2.3.2",
		"facepaint": "1.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-redux": "8.0.4",
